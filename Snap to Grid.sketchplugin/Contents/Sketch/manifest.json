{
  "name": "Snap to Grid",
  "description": "A Sketch plugin to automatically align artboards and layers to a grid",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-snap-to-grid",
  "version": "0.0.2",
  "identifier": "yuanqing.sketch-snap-to-grid",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-snap-to-grid/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Snap Layers to Grid",
      "identifier": "yuanqing.sketch-snap-to-grid.snap-layers-to-grid.snap-layers-to-grid",
      "script": "plugin.js",
      "handler": "snap-layers-to-grid/snap-layers-to-grid"
    },
    {
      "name": "Settings for Snap Layers to Grid",
      "identifier": "yuanqing.sketch-snap-to-grid.snap-layers-to-grid.settings",
      "script": "plugin.js",
      "handler": "snap-layers-to-grid/settings"
    },
    {
      "name": "Snap Artboards to Grid",
      "identifier": "yuanqing.sketch-snap-to-grid.snap-artboards-to-grid.snap-artboards-to-grid",
      "script": "plugin.js",
      "handler": "snap-artboards-to-grid/snap-artboards-to-grid"
    },
    {
      "name": "Settings for Snap Artboards to Grid",
      "identifier": "yuanqing.sketch-snap-to-grid.snap-artboards-to-grid.settings",
      "script": "plugin.js",
      "handler": "snap-artboards-to-grid/settings"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-snap-to-grid.reset-settings",
      "script": "plugin.js",
      "handler": "reset-settings"
    },
    {
      "name": "action",
      "identifier": "yuanqing.sketch-snap-to-grid.action.layers-moved-finish",
      "script": "plugin.js",
      "handlers": {
        "actions": {
          "LayersMoved.finish": "action"
        }
      }
    },
    {
      "name": "action",
      "identifier": "yuanqing.sketch-snap-to-grid.action.layers-resized-finish",
      "script": "plugin.js",
      "handlers": {
        "actions": {
          "LayersResized.finish": "action"
        }
      }
    },
    {
      "name": "action",
      "identifier": "yuanqing.sketch-snap-to-grid.action.paste-finish",
      "script": "plugin.js",
      "handlers": {
        "actions": {
          "Paste.finish": "action"
        }
      }
    }
  ],
  "menu": {
    "title": "Snap to Grid",
    "items": [
      "yuanqing.sketch-snap-to-grid.snap-layers-to-grid.snap-layers-to-grid",
      "yuanqing.sketch-snap-to-grid.snap-layers-to-grid.settings",
      "-",
      "yuanqing.sketch-snap-to-grid.snap-artboards-to-grid.snap-artboards-to-grid",
      "yuanqing.sketch-snap-to-grid.snap-artboards-to-grid.settings",
      "-",
      "yuanqing.sketch-snap-to-grid.reset-settings"
    ]
  }
}
